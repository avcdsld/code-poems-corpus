        <l xml:id="l1">function disfunction(){</l>
        <l xml:id="l2"></l>
        <l xml:id="l3">if(weCannotStart&amp;&amp;weWillNotStop) {</l>
        <l xml:id="l4">if(iCannotRun&amp;&amp;uCannotWalk) { if(thereIsNoWay&amp;&amp;thereIsNoOne) { </l>
        <l xml:id="l5">if(uHoldYourFlag&amp;&amp;iHoldMyGun) { while(timePassesBy&amp;&amp;hopePassesOut) {</l>
        <l xml:id="l6">while(someRemainSilent&amp;&amp;someOnlyShout){</l>
        <l xml:id="l7">while(someWaitForJesus&amp;&amp;someWaitForFood){</l>
        <l xml:id="l8">while(uChangeYouClothes&amp;&amp;iChangeMyMood){</l>
        <l xml:id="l9">try{settlingDown||openingUp;</l>
        <l xml:id="l10">//try</l>
        <l xml:id="l11">listeningIn||!actingOut;</l>
        <l xml:id="l12">//try</l>
        <l xml:id="l13">[anything,something,whatever,whoCares];}</l>
        <l xml:id="l14">//all that's left when you</l>
        <l xml:id="l15">catch(me){isAskingWhosThere;}}}}}}}}}}</l>
      </div>
