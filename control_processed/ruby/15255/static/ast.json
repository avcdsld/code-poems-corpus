  "text": "def code(*colors)\n      attribute = []\n      colors.each do |color|\n        value = ANSI::ATTRIBUTES[color] || ALIASES[color]\n        if value\n          attribute << value\n        else\n          validate(color)\n        end\n      end\n      attribute\n    end",
