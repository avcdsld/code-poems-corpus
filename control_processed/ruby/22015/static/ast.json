  "text": "def migrate!\n      @semaphore.synchronize do\n        return unless @active\n\n        networks = @core.config.networks\n        (@checkpoint - networks).each { |network| disconnect_from network }\n        (networks - @checkpoint).each { |network| connect_to      network }\n        @checkpoint = networks\n      end\n    end",
