  "text": "def option_as_array(option)\n      value = Merb::Plugins.config[:exceptions][option]\n      case value\n      when Array\n        value.reject { |e| e.nil? } # Don't accept nil values\n      when String\n        [value]\n      else\n        []\n      end\n    end",
