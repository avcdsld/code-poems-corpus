  "text": "def destroy(force = false)\n      if (!force && file =~ /\\.yardoc$/) || force\n        if File.file?(@file)\n          # Handle silent upgrade of old .yardoc format\n          File.unlink(@file)\n        elsif File.directory?(@file)\n          FileUtils.rm_rf(@file)\n        end\n        true\n      else\n        false\n      end\n    end",
