  "text": "def authorize user\n      auth_conv = HaystackRuby::Auth::Scram::Conversation.new(user, @url)\n      auth_conv.authorize\n      @auth_token = auth_conv.auth_token\n      raise HaystackRuby::Error, \"scram authorization failed\" unless @auth_token.present?\n    end",
