  "text": "def transition_running_state(target_state)\n      state_transitions = {\n        not_started: :running,\n        running: :stopping,\n        stopping: :stopped\n      }\n      if state_transitions[@running_state] == target_state\n        @running_state = target_state\n      else\n        fail \"Bad server state transition: #{@running_state}->#{target_state}\"\n      end\n    end",
