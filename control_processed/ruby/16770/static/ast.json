  "text": "def sanitize_cookies(env)\n      return unless env['HTTP_COOKIE']\n\n      env['HTTP_COOKIE'] = env['HTTP_COOKIE']\n        .split(/[;,] */n)\n        .map { |cookie| sanitize_uri_encoded_string(cookie) }\n        .join('; ')\n    end",
