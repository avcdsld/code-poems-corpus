  },
  "text": "def align(a)\n      case a\n      when 1\n        nil\n      when 2, 4, 8\n        bits = a - 1\n        @idx = @idx + bits & ~bits\n        raise IncompleteBufferException if @idx > @buffy.bytesize\n      else\n        raise \"Unsupported alignment #{a}\"\n      end\n    end",
